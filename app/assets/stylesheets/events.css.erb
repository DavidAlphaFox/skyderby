/*Общие стили для событий*/

.event-request { margin-top:15px }

.template-row {
  display: none;
}

.edit-competitor {
  margin-left: 10px;
}
.delete-competitor {
  margin-left: 5px;
}

td .edit-competitor {
  visibility: hidden;
}

td:hover .edit-competitor {
  visibility: visible;
}

td .delete-competitor {
  visibility: hidden;
}

td:hover .delete-competitor {
  visibility: visible;
}